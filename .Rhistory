ErrorMat <- function(frame, control, Epsi){
Unsimplified <- control
Simplified <- DPFreDistances(frame, Epsi)
ErrorMatrix <- abs(((Simplified - Unsimplified) / Unsimplified))
return(ErrorMatrix)
}
